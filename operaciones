void main()
{
	int A,B,C,D,F;
	cout <<"ingrese un valor"<<endl;
	cin >>A;
	cout<<"ingrese otro valor"<<endl;
	cin>>B;
	cout<<"ingrese otro valor"<<endl;
	cin>>D;
	cout<<"ingrese otro valor"<<endl;
	cin>>F;
